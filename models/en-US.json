{
  "invocation": "produce pal",
  "intents": [
    {
      "name": "AddFoodIntent",
      "phrases": ["add {food} to my fridge", "add {food}", "put {food} away"],
      "inputs": [
        {
          "name": "food",
          "type": {
            "alexa": "AMAZON.Food"
          }
        }
      ]
    },
    {
      "name": "SaveFoodIntent",
      "phrases": ["{days} days", "it will be good for {days} days"],
      "inputs": [
        {
          "name": "days",
          "type": {
            "alexa": "AMAZON.NUMBER"
          }
        }
      ]
    },
    {
      "name": "ServingCountIntent",
      "phrases": ["{servings} servings"],
      "inputs": [
        {
          "name": "servings",
          "type": {
            "alexa": "AMAZON.NUMBER"
          }
        }
      ]
    },
    {
      "name": "ListFoodIntent",
      "phrases": ["What is in my fridge?", "What food do I have?"]
    },
    {
      "name": "QuickAddIntent",
      "phrases": ["add {servings} servings of {food} good for {days} days"],
      "inputs": [
        {
          "name": "servings",
          "type": {
            "alexa": "AMAZON.NUMBER"
          }
        },
        {
          "name": "food",
          "type": {
            "alexa": "AMAZON.Food"
          }
        },
        {
          "name": "days",
          "type": {
            "alexa": "AMAZON.NUMBER"
          }
        }
      ]
    }
  ]
}