{
  "invocation": "produce pal",
  "intents": [
    {
      "name": "AddFoodIntent",
      "phrases": ["add {food} to my fridge", "add {food}", "put {food} away"],
      "inputs": [
        {
          "name": "food",
          "type": {
            "alexa": "AMAZON.Food"
          }
        }
      ]
    },
    {
      "name": "SaveFoodIntent",
      "phrases": ["{number} days", "it will be good for {number} days"],
      "inputs": [
        {
          "name": "number",
          "type": {
            "alexa": "AMAZON.NUMBER"
          }
        }
      ]
    },
    {
      "name": "ServingCountIntent",
      "phrases": ["{number} servings"],
      "inputs": [
        {
          "name": "number",
          "type": {
            "alexa": "AMAZON.NUMBER"
          }
        }
      ]
    },
    {
      "name": "ListFoodIntent",
      "phrases": ["What is in my fridge?", "What food do I have?"]
    }
  ]
}